{
  "name": "omarayman/dictionary",
  "description": "It is a package that facilitates the interaction with some of the known dictionary apis like google for example",
  "license": "MIT",
  "keywords": [
    "Dictionary",
    "Google Dictionnary"
  ],
  "authors": [
    {
      "name": "Omar Ayman",
      "email": "oa36345@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/config": "5.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*",
    "mockery/mockery": "0.*",
    "symfony/var-dumper": "2.*"
  },
  "autoload": {
    "psr-4": {
      "Omarayman\\Dictionary\\": "src",
      "Nextpack\\Library\\": "src/library"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Omarayman\\Dictionary\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "stable"
}
